cat("List of tracked anime:")
for (i in c(1:length(tracked.anime))) {cat("\n",i, " ", tracked.anime[i])}
last.operation <- "\"get list of tracked anime\""
}
#3 - search for tracked anime in user's tag
#print those with highest wishlist

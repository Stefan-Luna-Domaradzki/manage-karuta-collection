names(uc.split.nw)[1] <- "none"
uc.menu <- uc.split.nw
last.operation <- "\"load new collection\""
}
#when adding workers optimalisation function will have following line
#uc.menu <- uc.split.ww
#2 - get list of tracked anime
if(user.input == "2") {
